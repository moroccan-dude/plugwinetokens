<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.bytecode.use_reflection_optimizer">false</property> 
        <property name="hibernate.search.autoregister_listeners">false</property>
        <mapping resource="hibernate/Component.hbm.xml" />
        <mapping resource="hibernate/Stage.hbm.xml" />
        <mapping resource="hibernate/ReleasePath.hbm.xml" />
        <mapping resource="hibernate/ConfigurationVariableValue.hbm.xml" />
        <mapping resource="hibernate/ApplicationVersionStageActivity.hbm.xml" />
        <mapping resource="hibernate/ApplicationVersion.hbm.xml" />
        <mapping resource="hibernate/StageType.hbm.xml" />
        <mapping resource="hibernate/ApplicationVersionStage.hbm.xml" />
        <mapping resource="hibernate/ConfigurationVariable.hbm.xml" />
    </session-factory>
</hibernate-configuration>
